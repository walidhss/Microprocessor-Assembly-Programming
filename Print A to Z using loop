.model small
.stack 100h
.data
msg db 'Loop concept$'
.code
main proc
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,msg
    int 21h
    
    mov ah,2
    mov dl,10 
    int 21h
    mov dl,13
    int 21h
    
    mov cx,26         //loop counter
    mov ah,2
    mov dl,'A'        //ASCII for 'A'
    
    level1:
    int 21h         //print character in DL
    inc dl          //next character
    loop level1     //decrement CX, repeat until CX = 0
    
    exit:
    mov ah,4ch
    int 21h
    main endp
end main

